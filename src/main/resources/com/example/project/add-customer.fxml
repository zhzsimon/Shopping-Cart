<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="271.0" prefWidth="420.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project.AddCustomerController">
   <children>
      <VBox alignment="CENTER" layoutX="14.0" layoutY="14.0" prefHeight="248.0" prefWidth="398.0">
         <children>
            <HBox alignment="CENTER" prefHeight="50.0" prefWidth="398.0">
               <children>
                  <Label prefHeight="15.0" prefWidth="73.0" text="Gender:">
                     <font>
                        <Font size="15.0" />
                     </font></Label>
                  <ChoiceBox fx:id="selectGender" prefWidth="150.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="55.0" prefWidth="398.0">
               <children>
                  <Label prefWidth="73.0" text="Age:">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <TextField fx:id="age" prefHeight="23.0" prefWidth="153.0" />
               </children>
            </HBox>
            <Button fx:id="enterBtn" mnemonicParsing="false" onAction="#enterBtnClicked" prefHeight="29.0" prefWidth="46.0" text="Enter">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></Button>
            <Label fx:id="message" textFill="#f50606">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children></VBox>
   </children>
</AnchorPane>
